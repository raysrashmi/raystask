<header>
  <form>
    <input type='text' class='task-title'/>
    <input type='submit'/>
  </form>
</header>

<ul class='tasks'>
  <% for task in @tasks.models: %>
    <li>
      <dl>
        <dt>
          <input type='checkbox'/>
          <%= task.get('title') %>
        </dt>
      </dl>
    </li>
  <% end %>
</ul>
